<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="BlackListedExtensions" value=".svg,.png,.jpg,.gif,.bmp,.tga,.pcx" /> <!-- Comma separated list including period and extension example: .jpg,.png -->
    <add key="SkipHiddenFolders" value="1" /> <!-- 1 == true ... 0 == false -->
    <add key="SkipFoldersThatBeginWithPeriod" value="1" /><!-- 1 == true ... 0 == false -->
  </appSettings>
</configuration>